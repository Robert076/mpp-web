datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Gun {
  id             Int     @id @default(autoincrement())
  name           String
  caliber        Float
  weight         Float
  actionType     String
  category       String?
  effectiveRange Int?
}
